{
    "name": "laraxot/module_tenant_fila",
    "description": "tenant module for software architecture that allows a single instance of a software application to serve multiple tenants",
    "keywords": [
        "laraxot",
        "laravel",
        "filament",
        "module_tenant",
        "Laravel", 
        "Tenant module"
    ],
    "homepage": "https://github.com/laraxot/module_tenant_fila",
    "license": "MIT",
    "authors": [{
        "name": "Marco Xot",
        "email": "marco.sottana@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "laraxot/module_xot_fila": "dev-master",
        "thecodingmachine/safe": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "*",
        "fakerphp/faker": "*",
        "friendsofphp/php-cs-fixer": "*",
        "nesbot/carbon": "*",
        "nette/php-generator": "*",
        "nunomaduro/larastan": "*",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpunit/phpunit": "*",
        "psalm/plugin-laravel": "^2.8",
        "spatie/laravel-typescript-transformer": "*",
        "spatie/pest-plugin-snapshots": "*",
        "spatie/phpunit-snapshot-assertions": "*",
        "spatie/test-time": "*",
        "thecodingmachine/phpstan-safe-rule": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Tenant\\Providers\\TenantServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Tenant\\": ""
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
